插入排序
function insertionSort(array) {
  var len = array.length;
  for (var i = 0; i < len; i++) {
    var current = array[i];
    var j = i - 1;
    while (j >= 0 && array[j] > current) {
      array[j + 1] = array[j];
      j--;
    }
    array[j + 1] = current;
  }
  return array;
}


